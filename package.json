{
  "name": "vite-styleguidist",
  "description": "Styleguidist, using Vite ⚡️ with Vue 3 support",
  "version": "0.0.0",
  "main": "lib/index.ts",
  "types": "lib/index.d.ts",
  "files": [
    "lib/index.ts"
  ],
  "author": "Jessica Sachs <jess@jessicasachs.io>",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently --names=\"Server\" -c=\"magenta.bold,cyan.bold\" yarn:watch:server",
    "watch:server": "nodemon src/index.ts",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "test": "jest --config jest.config.json"
  },
  "dependencies": {
    "koa-json": "^2.0.2",
    "koa-router": "^8.0.8",
    "lodash": "^4.17.15",
    "vite": "^0.14.3",
    "vue": "next",
    "vue-router": "^4.0.0-alpha.11"
  },
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@types/koa": "^2.11.3",
    "@types/koa-json": "^2.0.18",
    "@types/koa-router": "^7.4.0",
    "@types/lodash": "^4.14.150",
    "@types/node": "^13.13.2",
    "concurrently": "^5.2.0",
    "jest": "^25.4.0",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.5",
    "ts-jest": "^25.4.0",
    "ts-node": "^8.10.1",
    "tslint": "^6.1.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.8.3"
  }
}
